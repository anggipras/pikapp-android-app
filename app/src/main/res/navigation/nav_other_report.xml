<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_other_report"
    app:startDestination="@id/homeReportFragment">

    <fragment
        android:id="@+id/homeReportFragment"
        android:name="com.tsab.pikapp.view.other.otherReport.HomeReportFragment"
        android:label="home_report_fragment"
        tools:layout="@layout/home_report_fragment" >
        <action
            android:id="@+id/action_homeReportFragment_to_filterPageReport"
            app:destination="@id/filterPageReport" />
        <action
            android:id="@+id/action_homeReportFragment_to_uploadReportFragment"
            app:destination="@id/uploadReportFragment"/>
    </fragment>
    <fragment
        android:id="@+id/filterPageReport"
        android:name="com.tsab.pikapp.view.other.otherReport.FilterPageReport"
        android:label="fragment_filter_page_report"
        tools:layout="@layout/fragment_filter_page_report" >
    </fragment>

    <fragment
        android:id="@+id/uploadReportFragment"
        android:name="com.tsab.pikapp.view.other.otherReport.UploadReportFragment"
        android:label="fragment_upload_report"
        tools:layout="@layout/fragment_upload_report" >
        <action
            android:id="@+id/action_uploadReportFragment_to_uploadStatusFragment"
            app:destination="@id/uploadStatusFragment" />
    </fragment>
    <fragment
        android:id="@+id/uploadStatusFragment"
        android:name="com.tsab.pikapp.view.other.otherReport.UploadStatusFragment"
        android:label="fragment_upload_status"
        tools:layout="@layout/fragment_upload_status" />
</navigation>