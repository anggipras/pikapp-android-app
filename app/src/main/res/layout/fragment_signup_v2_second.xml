<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="vertical"
    tools:context=".view.onboarding.login.SignupV2Second">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/header"
        android:layout_width="match_parent"
        android:layout_height="192dp"
        android:background="@drawable/header"
        app:layout_constraintBottom_toTopOf="@id/box"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <TextView
            android:id="@+id/judul1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/montserrat_bold"
            android:text="Registrasi Restoran"
            android:textColor="#ffffff"
            android:textSize="23dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.200"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.379" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <LinearLayout
        android:id="@+id/box"
        android:layout_width="321dp"
        android:layout_height="530dp"
        android:layout_marginTop="95dp"
        android:background="@drawable/border"
        android:elevation="50dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.497">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="15dp"
                android:fontFamily="@font/montserrat_bold"
                android:text="Nama Restoran"
                android:textColor="#000000"
                android:textSize="13dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/montserrat_bold"
                android:text="*"
                android:textColor="#DC6A84"
                android:textSize="13dp" />

        </LinearLayout>

        <EditText
            android:id="@+id/restaurant"
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="40dp"
            android:backgroundTint="#aaaaaa"
            android:inputType="text"
            android:fontFamily="@font/proximanova_regular"
            android:hint="Masukkan Nama Restoran Anda"
            android:textColorHint="#aaaaaa"
            android:textSize="13dp" />

        <TextView
            android:id="@+id/restauranterror"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:fontFamily="@font/proximanova_regular"
            android:text="Nama Lengkap"
            android:textColor="#ffffff"
            android:textSize="10dp" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/montserrat_bold"
                android:text="Nama Foodcourt"
                android:textColor="#000000"
                android:textSize="13dp" />

        </LinearLayout>

        <EditText
            android:id="@+id/foodcourt"
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="40dp"
            android:backgroundTint="#aaaaaa"
            android:fontFamily="@font/proximanova_regular"
            android:hint="Isi Jika Restoran Anda Berada Di Foodcourt"
            android:inputType="text"
            android:textColorHint="#aaaaaa"
            android:textSize="13dp" />

        <TextView
            android:id="@+id/fcourterror"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:fontFamily="@font/proximanova_regular"
            android:text="Nama Lengkap"
            android:textColor="#ffffff"
            android:textSize="10dp" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/montserrat_bold"
                android:text="Alamat"
                android:textColor="#000000"
                android:textSize="13dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/montserrat_bold"
                android:text="*"
                android:textColor="#DC6A84"
                android:textSize="13dp" />

        </LinearLayout>

        <EditText
            android:id="@+id/alamat"
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="40dp"
            android:backgroundTint="#aaaaaa"
            android:inputType="text"
            android:fontFamily="@font/proximanova_regular"
            android:hint="Contoh: AEON Mall, Jl. abcd No. 14"
            android:textColorHint="#aaaaaa"
            android:textSize="13dp" />

        <TextView
            android:id="@+id/alamaterror"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:fontFamily="@font/proximanova_regular"
            android:text="Nama Lengkap"
            android:textColor="#ffffff"
            android:textSize="10dp" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/montserrat_bold"
                android:text="Nama Bank"
                android:textColor="#000000"
                android:textSize="13dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/montserrat_bold"
                android:text="*"
                android:textColor="#DC6A84"
                android:textSize="13dp" />

        </LinearLayout>

        <EditText
            android:id="@+id/bank"
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="40dp"
            android:backgroundTint="#aaaaaa"
            android:drawableEnd="@drawable/ic_baseline_keyboard_arrow_down_24"
            android:fontFamily="@font/proximanova_regular"
            android:hint="Pilih Bank Yang Digunakan"
            android:textColorHint="#aaaaaa"
            android:textSize="13dp" />

        <TextView
            android:id="@+id/bankerror"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:fontFamily="@font/proximanova_regular"
            android:text="Nama Lengkap"
            android:textColor="#ffffff"
            android:textSize="10dp" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/montserrat_bold"
                android:text="Nomor Rekening Restoran"
                android:textColor="#000000"
                android:textSize="13dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/montserrat_bold"
                android:text="*"
                android:textColor="#DC6A84"
                android:textSize="13dp" />

        </LinearLayout>

        <EditText
            android:id="@+id/norek"
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="40dp"
            android:backgroundTint="#aaaaaa"
            android:fontFamily="@font/proximanova_regular"
            android:hint="Masukkan Nomor Rekening Bank"
            android:inputType="number"
            android:textColorHint="#aaaaaa"
            android:textSize="13dp" />

        <TextView
            android:id="@+id/norekerror"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:fontFamily="@font/proximanova_regular"
            android:text="Nama Lengkap"
            android:textColor="#ffffff"
            android:textSize="10dp" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/montserrat_bold"
                android:text="Nama Pemilik Rekening"
                android:textColor="#000000"
                android:textSize="13dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/montserrat_bold"
                android:text="*"
                android:textColor="#DC6A84"
                android:textSize="13dp" />

        </LinearLayout>

        <EditText
            android:id="@+id/namarek"
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="40dp"
            android:inputType="text"
            android:backgroundTint="#aaaaaa"
            android:fontFamily="@font/proximanova_regular"
            android:hint="Masukkan Nama Pemilik Rekening"
            android:textColorHint="#aaaaaa"
            android:textSize="13dp" />

        <TextView
            android:id="@+id/namarekerror"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:fontFamily="@font/proximanova_regular"
            android:text="Nama Lengkap"
            android:textColor="#ffffff"
            android:textSize="10dp" />

        <ImageButton
            android:id="@+id/btnNext"
            android:layout_width="307dp"
            android:layout_height="77dp"
            android:layout_gravity="center"
            android:layout_marginTop="0dp"
            android:background="@drawable/buttonext" />


    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>